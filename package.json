{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "yarn format && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --runInBand --verbose --detectOpenHandles --forceExit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test:coverage": "yarn test --coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "preinstall": "rm -rf .git/hooks/pre-push && rm -rf .git/hooks/pre-commit"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/masked-view": "0.1.5",
    "expo": "~36.0.0",
    "expo-font": "~8.0.0",
    "expo-linear-gradient": "^8.0.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-app-intro-slider": "^3.0.0",
    "react-native-dotenv": "^0.2.0",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-svg": "9.13.3",
    "react-native-web": "~0.11.7",
    "react-navigation": "^4.2.1",
    "react-navigation-stack": "^2.1.1",
    "styled-components": "^5.0.1"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/config/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/config"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@testing-library/react-native": "^5.0.3",
    "@types/jest": "^25.1.3",
    "@types/react": "~16.9.0",
    "@types/react-native": "~0.60.23",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/styled-components": "^4.4.3",
    "babel-preset-expo": "~8.0.0",
    "husky": "^4.2.3",
    "jest-expo": "^36.0.1",
    "jest-styled-components": "^7.0.0",
    "prettier": "^1.19.1",
    "reactotron-react-native": "^4.0.3",
    "typescript": "~3.6.3"
  },
  "private": true
}
